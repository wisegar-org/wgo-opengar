{
  "name": "wgo-agv-module",
  "version": "1.0.1-30",
  "description": "AGV App",
  "main": "build/index.js",
  "scripts": {
    "start": "nodemon",
    "build": "npx rimraf build && tsc",
    "build:promote": "npm run build && npm version prerelease",
    "update:wg-server": "npx rimraf ./node_modules@wisegar-org/wgo-server &&  npm install @wisegar-org/wgo-server@latest -E",
    "update:wg-settings": "npx rimraf ./node_modules@wisegar-org/wgo-settings &&  npm install @wisegar-org/wgo-settings@latest -E",
    "update:wg-obj-extensions": "npx rimraf ./node_modules@wisegar-org/wgo-object-extensions && npm install @wisegar-org/wgo-object-extensions@latest -E",
    "update-base": "rimraf node_modules/@wisegar-org/wgo-base-models && npm install @wisegar-org/wgo-base-models@latest -E",
    "update-base-server": "rimraf node_modules/@wisegar-org/wgo-base-server && npm install @wisegar-org/wgo-base-server@latest -E",
    "migration:run": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:run",
    "migration:generate": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:generate",
    "migration:revert": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:revert",
    "migration:create": "node --require ts-node/register ./node_modules/typeorm/cli.js migration:create",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Wisegar.org Development Group",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "9.0.13",
    "@types/graphql-upload": "^8.0.11",
    "@types/node": "^18.11.12",
    "@types/uuid": "^9.0.0",
    "nodemon": "2.0.16",
    "ts-node": "10.8.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "@types/bcrypt": "5.0.0",
    "@wisegar-org/wgo-base-models": "0.0.1-8",
    "@wisegar-org/wgo-base-server": "0.0.1-12",
    "@wisegar-org/wgo-mailer": "0.0.1-build.24",
    "@wisegar-org/wgo-object-extensions": "0.0.2-2",
    "@wisegar-org/wgo-server": "0.0.1-build.54",
    "@wisegar-org/wgo-settings": "0.0.1-build.9",
    "@wisegar-org/wgo-templating": "0.0.1-build.3",
    "bcrypt": "^5.1.0",
    "currency.js": "2.0.4",
    "dayjs": "^1.11.7",
    "express": "4.18.1",
    "fs-extra": "10.1.0",
    "graphql": "^15.3.0",
    "inline-css": "4.0.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "1.1.1",
    "typeorm": "0.3.6",
    "typeorm-extension": "2.1.2",
    "uuid": "^9.0.0"
  }
}
